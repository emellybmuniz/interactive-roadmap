{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/server.js"
      },
      {
        "source": "/css/(.*)",
        "destination": "/css/$1"
      },
      {
        "source": "/favicon.ico",
        "destination": "/favicon.ico"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  